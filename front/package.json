    "chart.js": "^2.7.3",
    "chartjs-plugin-labels": "^1.1.0",
    "nuxt": "^2.0.0",
    "vue-chartjs": "^3.4.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.6.3",
    "babel-eslint": "^10.0.1",
    "eslint": "^4.19.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-vue": "^4.0.0"
  }
}
