{
  "name": "front",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "mitsudama",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "start": "nuxt start",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "build:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt build",
